<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %>
<body>
<%- include('./partials/nav.ejs') %>
<div class="contacts-page">
    <h1 class="contacts-page__title">My contacts</h1>
    <ul class="contacts-page__list">
        <% if (contacts.length) { %>
            <% contacts.forEach(({link, name}) => { %>
                <li>
                    <a href="<%= link %>"><%= name %></a>
                </li>
            <% }) %>
        <% } %>
    </ul>
</div>
</body>
</html>
